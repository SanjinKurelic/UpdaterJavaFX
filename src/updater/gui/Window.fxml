<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="updater.gui.WindowController">
   <children>
      <VBox prefHeight="400.0" prefWidth="400.0" style="-fx-background-color: #F0F0F0;">
         <children>
            <Label prefHeight="80.0" prefWidth="400.0" style="-fx-background-color: #009688;" text="Updater" textFill="#f0f0f0">
               <font>
                  <Font name="Roboto Regular" size="27.0" />
               </font>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <padding>
                  <Insets left="10.0" />
               </padding>
            </Label>
            <ScrollPane hbarPolicy="NEVER" prefHeight="280.0" prefWidth="200.0" vbarPolicy="ALWAYS">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0">
                     <children>
                        <VBox fx:id="list" prefWidth="365.0" />
                     </children>
                  </AnchorPane>
              </content>
               <VBox.margin>
                  <Insets left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </ScrollPane>
            <HBox alignment="CENTER_RIGHT" prefHeight="60.0" prefWidth="400.0" style="-fx-background-color: #F0F0F0;">
               <children>
                  <Button fx:id="close" mnemonicParsing="false" onAction="#close" prefWidth="80.0" style="-fx-border-width: 0; -fx-background-color: #DEDEDE; -fx-border-radius: 0;" text="CLOSE" textFill="#424242">
                     <font>
                        <Font name="Roboto Regular" size="14.0" />
                     </font>
                     <HBox.margin>
                        <Insets right="10.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="update" mnemonicParsing="false" onAction="#update" prefWidth="80.0" style="-fx-background-color: #009688; -fx-border-radius: 0; -fx-border-width: 0;" text="UPDATE" textFill="#f0f0f0">
                     <font>
                        <Font name="Roboto Regular" size="14.0" />
                     </font>
                     <HBox.margin>
                        <Insets right="10.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
